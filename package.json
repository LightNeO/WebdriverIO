{
  "name": "WebdriverIO",
  "type": "module",
  "version": "1.0.0",
  "description": "Comprehensive test automation framework using WebdriverIO with Page Object Model",
  "main": "index.js",
  "scripts": {
    "wdio": "wdio run ./wdio.conf.js",
    "test": "wdio run ./wdio.conf.js",
    "test:login": "wdio run ./wdio.conf.js --spec ./test/specs/login.test.js",
    "test:registration": "wdio run ./wdio.conf.js --spec ./test/specs/registration.test.js",
    "test:product": "wdio run ./wdio.conf.js --spec ./test/specs/product.test.js",
    "test:headless": "wdio run ./wdio.conf.js --headless",
    "report:generate": "allure generate allure-results --clean",
    "report:open": "allure open",
    "report:serve": "allure serve allure-results",
    "lint": "eslint test/",
    "lint:fix": "eslint test/ --fix"
  },
  "keywords": [
    "webdriverio",
    "test-automation",
    "page-object-model",
    "selenium",
    "e2e-testing",
    "javascript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@wdio/cli": "^9.18.4",
    "@wdio/local-runner": "^9.18.4",
    "@wdio/mocha-framework": "^9.18.0",
    "@wdio/spec-reporter": "^9.18.0",
    "@wdio/allure-reporter": "^9.18.0",
    "allure-commandline": "^2.27.0",
    "webdriverio": "^9.18.4",
    "chai": "^4.3.10",
    "eslint": "^8.57.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/webdriverio-test-framework.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/webdriverio-test-framework/issues"
  },
  "homepage": "https://github.com/yourusername/webdriverio-test-framework#readme"
}
